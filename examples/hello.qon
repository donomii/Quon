(
 (includes base.qon)
 (types)
    
 (functions

  (void say (string greeting int count) (declare)
        (body
         (if (> count 0)
             (then 
              (printf "%s" greeting)
              (say greeting (sub1 count)))
             (else
              (return)))
         ))

  (int start () (declare)
       (body
        (say "Hello world\n" 3)
        (return 0)
        ))
  ))
