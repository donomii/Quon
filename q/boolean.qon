((includes)
 (types)
 (functions
	(bool not ( bool a)  (declare)
		(body
			(if a 
			(then (return false))
			(else (return true)))
	))

	(bool andBool ( bool a bool b)  (declare)
		(body
			(if a 
			(then 
				(if b
					(then (return true))
					(else (return false))))
			(else (return false)))
	))

	(bool nand (bool a bool b) (declare)
		(body
		(return (not (andBool a b)))
	))
	
	(bool xor (bool a bool b) (declare)
		(body
			(return (nand (nand a (nand a b)) (nand b (nand a b)))))
))